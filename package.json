{
  "name": "dependabot",
  "license": "MIT",
  "version": "1.0.1",
  "description": "Test packages version",
  "main": "index.js",
  "repository": "https://github.com/CedricAgoda/dependabot",
  "author": "CedricAgoda",
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "google-libphonenumber": "^3.2.10",
    "libphonenumber-js-utils": "^8.10.5",
    "moment": "^2.23.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-error-overlay": "^5.1.4",
    "react-intl-tel-input": "^8.0.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "4.3.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.8",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/reactstrap": "^6.4.4",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.0",
    "@types/styled-components": "^4.1.5",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-const-enum": "0.0.5",
    "babel-preset-es2015": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "copy-to-clipboard": "^3.2.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "error-overlay-webpack-plugin": "^0.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocker-api": "^1.6.7",
    "node-sass": "^4.12.0",
    "react": "^16.8.0",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.7.0",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-react": "^3.6.0",
    "tslint-refactor": "^0.1.0",
    "typescript": "3.4.5",
    "webpack": "^4.17.2",
    "webpack-api-mocker": "^1.6.6",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-merge": "^4.2.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/typescript"
    ],
    "plugins": [
      "const-enum",
      [
        "@babel/plugin-transform-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ],
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.mock.ts$",
      "\\.styled"
    ],
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 78,
        "functions": 70,
        "lines": 85,
        "statements": 85
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@agoda|react-intl-tel-input|libphonenumber-js-utils)/)"
    ],
    "testRegex": ".*\\.(test.ts|test.tsx)$",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "resolutions": {
    "write-file-atomic": "2.4.2"
  }
}
